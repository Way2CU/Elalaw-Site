<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../units/schema/template.dtd">
<document>
        <cms:var name="_DOCTYPE"/>
        <html lang="$language" cms:eval="lang">
            <!-- this is home -->
                <head>
                        <title><cms:text constant="site_title"/></title>
                        <cms:collection include="page_control,animation_chain"/>
                        <cms:module name="head_tag" action="print_tag"/>
                </head>

                <body class="rtl">
                        <cms:template file="parts/header.xml"/>
                        <!-- Content Start-->
                                <div id="content">
                                   <div class="inner_wrap">
                                    <cms:desktop>
                                        <div class="home_menu_container">
                                                <a
                                                href="url_Make('', 'about')"
                                                class="home_menu"
                                                cms:eval="href"
                                                >
                                                        <span class="menu_img first"></span>
                                                        <span class="title">
                                                                <cms:text constant="about"/>
                                                        </span>
                                                        <span class="sub_title">
                                                                <cms:text constant="home_about"/>
                                                        </span>
                                                </a>
                                                <a
                                                href="url_Make('', 'areas-of-practice')"
                                                class="home_menu"
                                                cms:eval="href"
                                                >
                                                        <span class="menu_img second"></span>
                                                        <span class="title">
                                                                <cms:text constant="thumei_isuk"/>
                                                        </span>
                                                        <span class="sub_title">
                                                                <cms:text constant="home_ti"/>
                                                        </span>
                                                </a>
                                                <a
                                                href="url_Make('', 'articles')"
                                                class="home_menu"
                                                cms:eval="href"
                                                >
                                                        <span class="menu_img third"></span>
                                                        <span class="title">
                                                                <cms:text constant="shut"/>
                                                        </span>
                                                        <span class="sub_title">
                                                                <cms:text constant="title"/>
                                                        </span>
                                                </a>
                                                <a
                                                href="url_Make('', 'articles')"
                                                class="home_menu"
                                                cms:eval="href"
                                                >
                                                        <span class="menu_img fourth"></span>
                                                        <span class="title">
                                                                <cms:text constant="articles"/>
                                                        </span>
                                                        <span class="sub_title">
                                                                <cms:text constant="home_articles"/>
                                                        </span>
                                                </a>
                                                <a
                                                href="url_Make('', 'contact')"
                                                class="home_menu"
                                                cms:eval="href"
                                                >
                                                        <span class="menu_img fifth"></span>
                                                        <span class="title">
                                                                <cms:text constant="contact"/>
                                                        </span>
                                                        <span class="sub_title">
                                                                <cms:text constant="home_contact"/>
                                                        </span>
                                                </a>
                                        </div>
                                        <div class="two_third">
                                            <cms:module
                                                name="articles"
                                                action="show"
                                                text_id="about_home"
                                                template="parts/article_force_inline.xml"
                                            />
                                        </div>
                                        </cms:desktop>
                                        <cms:mobile>
                                            <div class="two_third">
                                                <cms:module
                                                    name="articles"
                                                    action="show"
                                                    text_id="about_home"
                                                    template="parts/article_force_inline.xml"
                                                />
                                            </div>
                                           
                                            <div class="one_third">
                                                <h1 class="thumbnail">
                                                    <cms:text constant="center_text_list"/>
                                                </h1>
                                                <cms:module
                                                    name="articles"
                                                    action="show"
                                                    text_id="list_home"
                                                    template="parts/article_force_inline_noh1.xml"
                                                />
                                            </div>
                                        </cms:mobile>
                                    <!-- end of inner wrap content -->
                                   </div>
                                 <!-- Content End -->
                        <cms:template file="parts/footer.xml"/>
                </body>
        </html>
</document>